<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane minHeight="369.0" minWidth="444.0" prefHeight="369.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.ViewController">
   <children>
      <Pane fx:id="videoPlayer" prefHeight="311.0" prefWidth="444.0">
         <children>
            <MediaView fx:id="view" fitHeight="300.0" fitWidth="444.0" preserveRatio="false">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </MediaView>
         </children></Pane>
      <ButtonBar fx:id="playOptions" layoutX="27.0" layoutY="297.0" prefHeight="46.0" prefWidth="354.0">
        <buttons>
            <Button fx:id="timeBack" layoutX="189.0" layoutY="20.0" mnemonicParsing="false" onAction="#videoPlay" prefHeight="34.0" text="&lt;&lt; 10" />
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#playVideo" prefHeight="28.0" prefWidth="166.0" style="-fx-background-color: #4A5156; -fx-border-color: #FFFFFF; -fx-background-radius: 8;" text="Play" textFill="WHITE">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="stopButton" layoutX="125.0" layoutY="20.0" mnemonicParsing="false" onAction="#videoPlay" prefHeight="33.0" text="Stop" />
            <Button fx:id="timeForward" layoutX="189.0" layoutY="20.0" minWidth="75.0" mnemonicParsing="false" onAction="#videoPlay" prefHeight="34.0" text="10 &gt;&gt;" />
        </buttons>
      </ButtonBar>
      <Text fx:id="errorText" layoutX="49.0" layoutY="358.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="281.0" />
      <Button fx:id="muteButton" layoutX="339.0" layoutY="341.0" mnemonicParsing="false" onAction="#videoPlay" prefHeight="26.0" prefWidth="78.0" text="Mute" />
   </children>
</AnchorPane>
