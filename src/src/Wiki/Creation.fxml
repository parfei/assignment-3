      <Label fx:id="whatDoYouWant" layoutX="334.0" layoutY="98.0" prefHeight="16.0" prefWidth="594.0" text="Hi, What do you want to search" />
      <TextField fx:id="yourKeyWord" layoutX="152.0" layoutY="136.0" prefHeight="26.0" prefWidth="594.0" promptText="Your Keyword" />
      <Button fx:id="enterButton" layoutX="793.0" layoutY="136.0" mnemonicParsing="false" onAction="#search" text="search" />
      <Button fx:id="goingBack" layoutX="830.0" layoutY="360.0" mnemonicParsing="false" onAction="#backToMain" text="Back" />
      <ProgressBar fx:id="progress" layoutX="328.0" layoutY="244.0" prefWidth="200.0" progress="0.0" />
   </children>
</AnchorPane>
