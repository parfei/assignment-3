<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="labelask" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreationController">
